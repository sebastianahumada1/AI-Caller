{
  "name": "vapi-ghl-connector",
  "version": "1.0.0",
  "description": "Production-ready Vapi to GoHighLevel webhook connector",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:ngrok": "node scripts/dev-with-ngrok.js",
    "dev:manual": "node scripts/start-manual.sh",
    "build": "tsc",
    "vercel-build": "tsc",
    "vercel:check": "node scripts/vercel-check.js",
    "start": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "docker:build": "docker build -t vapi-ghl-connector .",
    "docker:run": "docker run -p 3000:3000 --env-file .env vapi-ghl-connector",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "docker:down:dev": "docker-compose -f docker-compose.dev.yml down",
    "test:slack": "node scripts/test-slack-integration.js"
  },
  "keywords": [
    "vapi",
    "gohighlevel",
    "webhook",
    "connector"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/form-data": "^2.2.1",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "optionalDependencies": {
    "@vercel/kv": "^1.0.1"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "tsx": "^4.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
